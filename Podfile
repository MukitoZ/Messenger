# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Messenger' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks! :linkage => :dynamic

  # Pods for Messenger

	#FireBase
	pod 'FirebaseCore'
	pod 'FirebaseAuth'
	pod 'FirebaseDatabase'
	pod 'FirebaseStorage'

	#Facebook
	pod 'FacebookCore'
	pod 'FacebookLogin'

	#Google
	pod 'GoogleSignIn'

	pod 'MessageKit'
	pod 'JGProgressHUD'
	pod 'RealmSwift'
	pod 'SDWebImage'

end
